<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body id="top">

    <div id="wrapper">

        <!-- HEADER -->
        <div id="header_wrap" class="outer">
            <header class="inner">
                <% include ../partials/header %>
            </header>
        </div>

        <!-- MAIN CONTENT -->
        <div id="main_content_wrap" class="outer">
            <section id="main_content" class="inner">
                <section id="dossier">
                    <h1><p><%= biomarkerName %></p></h1>

                    <p class="dossier_label">Type:&nbsp;&nbsp;</p><%= biomarkerInfo.Type %>

                    <br><br>
                    <p class="dossier_label">Alternate Names:&nbsp;&nbsp;</p>
                    <ul>
                        <% biomarkerInfo.Names.forEach(function(name) { %>
                            <li><%= name %></li>
                        <% }); %>
                    </ul>

                    <% if(biomarkerInfo["Drug_Name(if_FDA_Approved)"]) { %> <%= user.name %> <% } %>

                    <p class="dossier_label">State Change:&nbsp;&nbsp;</p><%= biomarkerInfo.State_Change %>

                    <br><br><br>
                </section>
            </section>
        </div>

        <!-- FOOTER  -->
        <div id="footer_wrap" class="outer">
            <footer class="inner">
                <% include ../partials/footer %>
            </footer>
        </div>

    </div><!-- END WRAPPER -->

</body>

<% include ../partials/navbar %>

</html>
